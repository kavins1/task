{
    "name": "sparkout/user",
    "description": "",
    "keywords": [
        "sparkout",
        "user"
    ],
    "homepage": "https://github.com/sparkout/user",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kavin S",
            "email": "kavin@sparkouttech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sparkout\\User\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sparkout\\User\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sparkout\\User\\UserServiceProvider"
            ],
            "aliases": {
                "User": "Sparkout\\User\\UserFacade"
            }
        }
    }
}
